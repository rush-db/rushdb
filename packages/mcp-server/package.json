{
  "name": "@rushdb/mcp-server",
  "description": "RushDB MCP Server - Model Context Protocol server for RushDB",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "mcp-server",
    "rushdb",
    "database",
    "graph-database",
    "neo4j",
    "llm",
    "ai-agents",
    "tools"
  ],
  "license": "Apache-2.0",
  "version": "1.17.0",
  "type": "module",
  "main": "./build/index.js",
  "bin": {
    "rushdb-mcp": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build:esbuild": "node esbuild.config.js",
    "build": "rimraf build && node esbuild.config.js --prod",
    "watch:esbuild": "node esbuild.config.js --watch",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@rushdb/javascript-sdk": "workspace:*",
    "@modelcontextprotocol/sdk": "1.20.0",
    "dotenv": "^16.4.7",
    "jsonschema": "^1.5.0"
  },
  "devDependencies": {
    "@rushdb/javascript-sdk": "workspace:*",
    "@modelcontextprotocol/inspector": "^0.17.0",
    "@types/node": "^20.17.23",
    "esbuild": "^0.25.0",
    "rimraf": "^6.0.1",
    "typescript": "5.7.2",
    "vitest": "^1.6.1"
  },
  "peerDependencies": {},
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "bugs": "https://github.com/rush-db/rushdb/issues",
  "repository": {
    "type": "https",
    "url": "https://github.com/rush-db/rushdb"
  }
}