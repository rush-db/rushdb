{
  "name": "@rushdb/javascript-sdk",
  "description": "RushDB Javascript SDK",
  "keywords": [
    "database",
    "graph database",
    "instant database",
    "instant-database",
    "instantdatabase",
    "instant db",
    "instant-db",
    "instantdb",
    "neo4j",
    "cypher",
