{
  "name": "@rushdb/javascript-sdk",
  "description": "RushDB Javascript SDK",
  "keywords": [
    "neo4j",
    "database",
    "firebase-alternative",
    "firebase",
    "cypher",
    "instant",
    "instantdb",
    "rushdb"
  ],
  "license": "Apache-2.0",
  "version": "0.6.0",
  "type": "module",
  "main": "cjs/index.cjs.node.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "pnpm clean && pnpm build-esm && pnpm build-cjs && pnpm postbuild",
    "build-esm": "mkdir -p esm && tsc -p tsconfig.esm.json",
    "build-cjs": "mkdir -p cjs && tsc -p tsconfig.cjs.json",
    "postbuild": "node scripts/post-build.js",
    "types:check": "tsc --noEmit",
    "clean": "rm -rf ./dist ./node_modules/.cache ./coverage ./esm ./cjs ./types",
    "flush": "rm -rf node_modules",
    "lint:circularDeps": "madge --circular ./src/*",
    "lint:circularDepsWithImage": "madge --image graph.svg --circular ./src/*"
  },
  "devDependencies": {},
  "peerDependencies": {},
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "bugs": "https://github.com/rush-db/rushdb/issues",
  "repository": {
    "type": "https",
    "url": "https://github.com/rush-db/rushdb"
  },
  "exports": {
    "types": "./types/index.d.ts",
    "browser": {
      "import": "./esm/index.esm.worker.js",
      "require": "./cjs/index.cjs.worker.js"
    },
    "bun": {
      "import": "./esm/index.esm.worker.js",
      "require": "./cjs/index.cjs.worker.js"
    },
    "deno": {
      "import": "./esm/index.esm.worker.js",
      "require": "./cjs/index.cjs.worker.js"
    },
    "worker": {
      "import": "./esm/index.esm.worker.js",
      "require": "./cjs/index.cjs.worker.js"
    },
    "workerd": {
      "import": "./esm/index.esm.worker.js",
      "require": "./cjs/index.cjs.worker.js"
    },
    "default": {
      "import": "./esm/index.esm.node.js",
      "require": "./cjs/index.cjs.node.js"
    }
  }
}
